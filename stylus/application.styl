body
  background: #EBEBEE;
  header
    width: 100%
    height 100px
    background: #fff
    .div
      height 100px
      width: 1200px
      margin: 0 auto
      position relative
      div
        width: 100%
        overflow hidden
        position absolute
        &.top
          top: 20px
        span 
          float left
          font-size: 14px;
          color: #999999;
          &.span1
            font-size: 20px;
            color: #333333;
            padding: 0 4px
          &.span
            padding: 0 4px
            margin-top: 4px
  main
    width: 1200px
    margin: 20px auto
    .header
      overflow hidden
      background: #fff
      height 150px
      position relative
      .err
        position: absolute
        height: 40px;
        line-height: 50px;
        top 96px
        left 40px
        min-width: 341px;
        background: url('../images/bg.png')
        background-size: 100% 100%
        font-size: 12px;
        color: #E1413E;
        text-align: center
        &.none
          display: none
      .div
        float left
        font-size: 18px;
        color: #333333;
        padding-top: 50px
      .div2
        float right
        margin: 40px 30px 0 0
        div
          float left
          &.first
            width: 94px
            .bg
              width: 100%
              height 36px
              background: url('../images/ic1.png')
              background-size: 100% 100%
            .font
              width: 100%
              font-size: 12px
              padding: 10px 0px
              color: #12CA96
          &.two
            width: 154px
            .bg
              width: 100%
              height 36px
              background: url('../images/ic2-1.png')
              background-size: 100% 100%
            .font
              width: 100%
              text-align: center
              font-size: 12px
              padding: 10px 2px
              color: #bbb
            &.green
              .bg
                background: url('../images/ic2-2.png')
                background-size: 100% 100%
              .font
                color: #12CA96
            &.red
              .bg
                background: url('../images/ic2-3.png')
                background-size: 100% 100%
              .font
                color: #E1413E
          &.three
            width: 154px
            .bg
              width: 100%
              height 36px
              background: url('../images/ic2-1.png')
              background-size: 100% 100%
            .font
              width: 100%
              text-align: center
              font-size: 12px
              padding: 10px 2px
              color: #bbb
            &.none
              display: none
            &.green
              .bg
                background: url('../images/ic2-2.png')
                background-size: 100% 100%
              .font
                color: #12CA96
            &.red
              .bg
                background: url('../images/ic2-3.png')
                background-size: 100% 100%
              .font
                color: #E1413E
          &.four
            width: 93px
            .bg
              width: 100%
              height 36px
              background: url('../images/ic3-1.png')
              background-size: 100% 100%
            .font
              width: 100%
              text-align: right
              font-size: 12px
              color: #bbb 
              padding: 10px 0px
            &.green
              .bg
                background: url('../images/ic3-2.png')
                background-size: 100% 100%
              .font
                color: #12CA96
            &.red
              .bg
                background: url('../images/ic3-3.png')
                background-size: 100% 100%
              .font
                color: #E1413E
    .div
      background: #fff
      position relative
      padding: 20px 40px
      img
        position absolute
        top 0
        left 0
      .tit
        font-size: 16px;
        color: #999999;
        font-weight: bold
      .com
        display: flex
        height 54px
        line-height 54px
        font-size: 14px;
        color: #333333;
        span 
          flex: 1
          text-align: center
          &.span1
            color: #999999;
          &.span2
            color: #E1413E;
        &.com1
          background: #F6F6F6;
          font-size: 14px;
          color: #666666;
          margin-top 20px
      .list
        border 1px solid #333
        margin-top: 30px
        .div
          ul
            li
              position relative
              list-style none
              height: 45px
              .div1
                width: 30px
                height 30px
                position absolute
                left -25px
                top 0
                span
                  position absolute
                  top -1px
                  display: block
                  width: 22px
                  height: 22px
                  background: url('../images/ic_nor_exhibition.png')
                  background-size: 100% 100%
                div
                  position absolute
                  top 20px
                  left 10px
                  border-left: 1px solid #D8D8D8
                  width: 1px
                  height 22px
              .div2
                color #999
                span
                  padding: 0 30px
                  font-size: 14px
              &.last
                .div2
                  color #333
                  font-weight: bold
                .div1
                  span
                    background: url('../images/ic_pre_exhibition.png')
                    background-size: 100% 100%
                  div
                    display: none
                &.fail
                  .div2
                    font-weight: bold
                    color: #E1413E
      table
        width: 100%
        margin-top: 30px
        border-bottom: 1px solid #333;
        border-right: 1px solid #333;
        tr
          td
            height 54px
            border-left: 1px solid #333;
            border-top: 1px solid #333;
            padding-left: 20px
            font-size: 14px;
            color: #999;
            &.td
              width: 15%
              color: #333
    .footer
      font-size: 16px;
      color: #333333;
      overflow hidden
      background: #fff
      padding-bottom: 20px
      span 
        float right
        &.num 
          font-size: 20px;
          color: #E1413E;
          font-weight: bold
          padding: 0 5px
          line-height 18px
      button
        float right
        background: #12CA96;
        border-radius: 21px;
        width: 140px
        height 42px
        border: none
        color #fff
        margin-top: 20px
        outline: none;
        margin-right: 40px;
