.main
  width: 1196px
  margin: 0 auto
  padding-bottom: 50px
  header
    width: 100%
    height: 52px
    background: url('../images/tab_bg.png')
    background-size: 100% 100%
    position relative
    text-align: center
    color: #333
    font-size: 20px
    font-weight: bold
    .span1
