  .main
    width 1200px
    height 60px
    line-height 60px
    margin 20px auto 0
    background: #fff
    position relative
    font-family: MicrosoftYaHei;
    font-size: 14px;
    color: #333333;
    div
      float left 
      &.div1
        width: 270px
        padding-left 40px
      &.div3
        float right
  .main1
    width 1120px
    padding 40px
    line-height 160px
    margin 0px auto
    background: #fff
    font-family: MicrosoftYaHei;
    overflow hidden
    img
      width: 287px
      float left
      margin-right: 60px
    span
      float left
      font-family: MicrosoftYaHei;
      font-size: 18px;
      color: #12CA96
    &.success
      img
        width: 287px
    &.code
      position relative
      .img
        width: 287px
      .codeImg
        position absolute
        width: 170px
        height: 170px
        margin-right: 0px
        left 150px
        top: 47px
  .recording
    width 1120px
    padding 40px
    margin 0px auto
    background: #fff
    font-family: MicrosoftYaHei;
    overflow hidden
    .div
      border-top 1px dashed #6666
      ul
        li
          position relative
          list-style none
          height: 45px
          .div1
            width: 30px
            height 30px
            position absolute
            left -25px
            top 0
            span
              position absolute
              top -1px
              display: block
              width: 22px
              height: 22px
              background: url('../images/ic_nor_exhibition.png')
              background-size: 100% 100%
            div
              position absolute
              top 20px
              left 10px
              border-left: 1px solid #D8D8D8
              width: 1px
              height 22px
          .div2
            color #999
            span
              padding: 0 30px
              font-size: 14px
          &.last
            .div2
              color #333
              font-weight: bold
            .div1
              span
                background: url('../images/ic_pre_exhibition.png')
                background-size: 100% 100%
              div
                display: none
            &.fail
              .div2
                font-weight: bold
                color: #E1413E
  .problem
    width 1120px
    padding 40px
    margin 0px auto
    ul
      li
        float left
        height 30px
        padding-right: 15px
        line-height 30px
        font-size: 14px;
        color: #333333;
        list-style none
        background: #fff
        border: 1px solid #E1E1E1;
        border-radius: 15px;
        margin: 20px
        span
          width: 48px
          height 30px
          text-align: center
          float left
          background: #D8D8D8;
          border-radius: 15px;
          margin-right 20px
  .ffoot
    width 1200px
    height 100px
    margin 80px auto 0
    button
      border: none
      color: #fff
      float right
      width: 140px
      height 42px
      background: #12CA96;
      border-radius: 21px;
