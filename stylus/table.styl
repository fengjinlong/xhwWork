  .main
    width 1200px
    margin 20px auto 0
    background: #fff
    position relative
    .i
      display: block
      width 10px
      height 10px
      position absolute
      top: 0
      left 0
    .title
      color #999999
      font-size: 16px
      font-weight: bold
      padding: 30px 40px 40px
    .div
      .div1
        width: 1120px
        height 135px
        margin: 0 auto
        border: 1px solid #333
        border-bottom none
        p
          margin: 0
          font-size: 14px;
          color: #333333;
          padding-left: 30px
          &.p1
            text-align: center
            font-size: 18px;
            font-weight: bold
            margin-top 30px
          &.p2
            margin-top 25px
          &.p3
            margin-top 10px
      table
        width: 1122px
        margin: 0 auto
        border-bottom: 1px solid #333
        tr
          height: 66px
          td 
            width 140px
            border-left: 1px solid #333
            border-top: 1px solid #333
            padding-left: 15px
            font-size: 14px;
            &.right
              border-right: 1px solid #333
            &.tit
              font-size: 18px;
              font-weight: bold

  .ffoot
    width 1200px
    height 100px
    margin 80px auto 0
    button
      border: none
      color: #fff
      float right
      width: 140px
      height 42px
      background: #12CA96;
      border-radius: 21px;
